# CrewHub API Server Configuration
# Copy this file to .env and fill in your actual values

# Server Configuration
PORT=3001
NODE_ENV=development
CORS_ORIGIN=http://localhost:3000,http://localhost:3001,http://localhost:3002,http://localhost:5173

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=boathouse_development
DB_USER=postgres
DB_PASSWORD=your_password_here
DB_SSL=false

# Authentication Configuration
JWT_SECRET=super-secret-jwt-key
DEFAULT_PIN=000000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000

# Logging
LOG_LEVEL=info

# Cache Configuration
CACHE_TTL=3600

# Service Integration
BOATHOUSE_ETL_URL=http://localhost:3000
ROWCALIBUR_BACKEND_URL=http://localhost:3002
ROWCALIBUR_FRONTEND_URL=http://localhost:5173